{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/torus/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/torus",
        "tsConfig": "packages/torus/tsconfig.lib.json",
        "project": "packages/torus/package.json",
        "entryFile": "packages/torus/src/index.ts",
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "compiler": "babel",
        "format": [
          "esm",
          "cjs"
        ],
        "assets": [
          {
            "glob": "packages/torus/README.md",
            "input": ".",
            "output": "."
          },
          {
            "glob": "packages/torus/assets/*",
            "input": ".",
            "output": "assets"
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/torus/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/torus"
      ],
      "options": {
        "jestConfig": "packages/torus/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    }
  },
  "tags": [
    "web3auth-wallet"
  ]
}
